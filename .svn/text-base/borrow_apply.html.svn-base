				  <div class="panel-body row">
				  	<!--form表单-->
				    <form class="form-horizontal col-sm-offset-1 col-sm-6">
				    	<!--第一个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">借款金额</label>
					    <div class="input-group col-sm-6">
					      <input type="text" class="form-control" id="exampleInputAmount">
					      <div class="input-group-addon">元</div>
						</div>
					  </div>
					  <!--第二个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">借款利息</label>
					    <div class="input-group col-sm-6">
					      <input type="text" class="form-control" id="exampleInputAmount">
					      <div class="input-group-addon">%</div>
						</div>
					  </div>
					  <!--第三个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">借款期限</label>
					    <div class="input-group col-sm-6">
					      <select class="form-control">
							  <option>1</option>
							  <option>3</option>
							  <option>6</option>
							  <option>9</option>
							  <option>12</option>
							  <option>24</option>
						 </select>
					      <div class="input-group-addon">月</div>
						</div>
					  </div>
					  <!--第四个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">借款利息</label>
					    <div class="input-group col-sm-6">
					      <label class="radio-inline">
  <input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1" checked="checked">按月分期 
</label>
					      <label class="radio-inline">
  <input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">按月到期
</label>
						</div>
					  </div>
					  <!--第五个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">最小投标</label>
					    <div class="input-group col-sm-6">
					      <input type="text" class="form-control" id="exampleInputAmount">
					      <div class="input-group-addon">元</div>
						</div>
					  </div>
					  <!--第六个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">投标奖金</label>
					    <div class="input-group col-sm-6">
					      <input type="text" class="form-control" id="exampleInputAmount">
					      <div class="input-group-addon">%</div>
						</div>
					  </div>
					  <!--第七个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">招标天数</label>
					    <div class="input-group col-sm-6">
					      <select class="form-control">
							  <option>1</option>
							  <option>2</option>
							  <option>3</option>
							  <option>4</option>
							  <option>5</option>
							  <option>6</option>
							  <option>7</option>
							  <option>8</option>
							  <option>9</option>
							  <option>10</option>
							  <option>11</option>
							  <option>12</option>
						 </select>
					      <div class="input-group-addon">月</div>
						</div>
					  </div>
					  <!--第八个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">借款标题</label>
					    <div class="input-group col-sm-9">
					      <input type="text" class="form-control" id="exampleInputAmount">
						</div>
					  </div>
					  <!--第九个-->
					  <div class="form-group row">
					    <label for="inputEmail3" class="col-sm-3 control-label">借款描述</label>
					    <div class="input-group col-sm-9">
					      <textarea class="form-control" rows="3" style="resize: none;"></textarea>
						</div>
					  </div>
					  <!--按钮-->
					  <div class="form-group">
					    <button class="btn btn-primary col-sm-offset-3">提交申请</button>
					  </div>
